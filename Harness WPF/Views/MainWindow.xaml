<Window x:Class="Harness_WPF.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Harness_WPF.Views"
        mc:Ignorable="d"
        Title="Pynių Generacija" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="53*"/>
            <ColumnDefinition Width="747*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Generuoti" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="Button_Click" Width="550" Grid.ColumnSpan="2"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,45,0,0" Grid.ColumnSpan="2"/>
    </Grid>
</Window>